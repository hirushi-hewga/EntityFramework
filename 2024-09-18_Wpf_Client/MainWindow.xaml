        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2024_09_18_Wpf_Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Click="Button_Click">Load Countries</Button>
            <Button Click="Button_Click_1">Load Artists</Button>
            <Button Click="Button_Click_2">Load Genres</Button>
            <Button Click="Button_Click_3">Load Albums</Button>
            <Button Click="Button_Click_4">Load Tracks</Button>
            <Button Click="Button_Click_5">Load Categories</Button>
